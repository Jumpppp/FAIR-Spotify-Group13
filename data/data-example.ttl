# Make the file an ontology so Protégé opens it cleanly
<https://w3id.org/FAIR-course-UT/2025-2026/group13/data>
    a owl:Ontology ;
    dcterms:title "Group 13 – Example data graph (no slashes in QNames)"@en .

######### Concept scheme + genres (use hyphens, not slashes) #########

ex:SpotifyGenres a skos:ConceptScheme ;
  skos:prefLabel "Spotify Genres"@en .

data:genre-pop a skos:Concept ;
  skos:prefLabel "pop"@en ;
  skos:inScheme ex:SpotifyGenres .

data:genre-mainstream a skos:Concept ;
  skos:prefLabel "mainstream"@en ;
  skos:inScheme ex:SpotifyGenres ;
  skos:broader data:genre-pop .

######### Artists #########

data:artist-lady-gaga a schema:Person ;
  schema:name "Lady Gaga" .

data:artist-bruno-mars a schema:Person ;
  schema:name "Bruno Mars" .

